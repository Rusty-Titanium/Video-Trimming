                <!-- closest working idea -->
                <Track x:Name="PART_Track" Grid.Row="1" VerticalAlignment="Bottom" Height="50" Margin="0 -30 0 0">
                    <Track.DecreaseRepeatButton>
                        <RepeatButton Command="{x:Static Slider.DecreaseLarge}" Style="{StaticResource RepeatButtonTransparent}"/>
                    </Track.DecreaseRepeatButton>
                    <Track.IncreaseRepeatButton>
                        <RepeatButton Command="{x:Static Slider.IncreaseLarge}" Style="{StaticResource RepeatButtonTransparent}"/>
                    </Track.IncreaseRepeatButton>
                    <Track.Thumb>
                        <Thumb x:Name="Thumb" Focusable="False" OverridesDefaultStyle="True" Template="{StaticResource SliderThumbHorizontalDefault}" VerticalAlignment="Center"/>
                    </Track.Thumb>
                </Track>


                <Track x:Name="PART_LeftTrack" Loaded="PART_LeftTrack_Loaded" Grid.Row="1" VerticalAlignment="Bottom" Height="50" Margin="0 -30 0 0">
                    <Track.Thumb>
                        <Thumb x:Name="LeftThumb" Focusable="False" OverridesDefaultStyle="True" Template="{StaticResource NoBarSliderThumbHorizontalDefault}" DragDelta="LeftThumb_DragDelta" VerticalAlignment="Center"/>
                    </Track.Thumb>
                </Track>

                <Track x:Name="PART_RightTrack" Loaded="PART_RightTrack_Loaded" Grid.Row="1" VerticalAlignment="Bottom" Height="50" Margin="0 -30 0 0">
                    <Track.Thumb>
                        <Thumb x:Name="RightThumb" Focusable="False" OverridesDefaultStyle="True" Template="{StaticResource NoBarSliderThumbHorizontalDefault}" DragDelta="RightThumb_DragDelta" VerticalAlignment="Center"/>
                    </Track.Thumb>
                </Track>
                
                
                
                
                
                
                
                
                
                
                
                
                
                
                <!-- original ->
                This track didn't have Height, VerticalAlignment, or Margin here at all.
                <Track x:Name="PART_Track" Grid.Row="1" VerticalAlignment="Bottom" Height="50" Margin="0 -30 0 0">
                    <Track.DecreaseRepeatButton>
                        <RepeatButton Command="{x:Static Slider.DecreaseLarge}" Style="{StaticResource RepeatButtonTransparent}"/>
                    </Track.DecreaseRepeatButton>
